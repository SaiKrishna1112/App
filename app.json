{
  "expo": {
    "name": "OxyLoans",
    "slug": "OxyLoans",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/oxy-loans.png",
    "splash": {
      "image": "./assets/oxy-loans.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 60000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/oxy-loans.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "App.Mobile"
    },
    "web": {
      "favicon": "./assets/icon23.png"
    }
  }
}
